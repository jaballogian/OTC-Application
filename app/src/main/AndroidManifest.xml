<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.otc.application">

    <application
        android:allowBackup="true"
        android:icon="@drawable/application_logo"
        android:label="@string/OTC"
        android:roundIcon="@drawable/application_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".ActivityTermsAndCondition"></activity>
        <activity android:name=".ActivityHome" />
        <activity android:name=".ActivitySignUp" />
        <activity android:name=".ActivitySignIn" />
        <activity android:name=".ActivityWelcomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>